<div class="container">
  <h1>Login</h1>
  <form>
    <input type="text" name="email" ng-model="user.email" placeholder="email"></input>
    <input type="password" name="password" ng-model="user.password" placeholder="password"></input>
    <button type="submit" ng-click="login()">Login</button>
  </form>
</div>